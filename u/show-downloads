==> map({
    ($_, qqx/cargo show $_/)
})

==> map({
    ($_[1].lines.grep({/downloads/}).split(" ")[1], $_[0])
})

==> sort({
    $^a[0].Num cmp $^b[0].Num
})

==> reverse()

==> map({
    .say
})
